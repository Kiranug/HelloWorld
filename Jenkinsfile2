@Library('jenkins-shared-library-framework@master') _ //master or whatever branch

pipeline {
    agent any  
    environment {
        // initialize common vars
        common_vars = staging.loadVars()
        var1 = staging.DB_URL()
        var2 = staging.DB_URL2()
        varA = var1 + "some_string"
        varB = var2 + "some_other_string"
     } // environment

    stages {
        stage('Some Stage') {
           steps {
                echo "env.var1 = ${env.var1}"
                echo "${env.var1}"
                echo "${env.var2}"
                
                 echo "${env.varA}"
                echo "${env.varB}"
            }
        }
    }

    post {
        always { 
            script {
                // Print environment variables
                sh "env"
            } // script
        } // always
    } // post

} // pipeline
